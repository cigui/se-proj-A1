<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.BorrowHistory" table="borrowinghistory">
		<id name="h_id" type="int">
			<column name="h_id" />
			<generator class="native" />
		</id>
    	<property name="id" type="java.lang.Integer"/> 
    	<property name="r_id" type="java.lang.Integer"/> 
    	<property name="date"/> 
    	<property name="location"/> 
    	<property name="orderStatus"/> 
    	<property name="score" type="java.lang.Byte"/> 
	</class>
</hibernate-mapping>