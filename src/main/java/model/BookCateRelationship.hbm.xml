<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.BookCateRelationship" table="bookcaterelationship">
		<composite-id>
			<key-property name="cate_id" column="cate_id" type="int" />
			<key-property name="isbn" column="isbn" type="long" />
		</composite-id>
	</class>
</hibernate-mapping>